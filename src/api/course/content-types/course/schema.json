{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "target_audience": {
      "type": "blocks"
    },
    "scope": {
      "type": "blocks"
    },
    "slug": {
      "type": "uid"
    },
    "isPopular": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "course"
    },
    "packages": {
      "type": "component",
      "repeatable": true,
      "component": "packages.package",
      "required": false
    }
  }
}
